N
N#define FLASH_ERASE_INDICATOR_INTERVAL      0x19876UL
N
N/*********************************************/
N
N
N/**********************************************
N*   Local variables and forward declarations  *
N**********************************************/
N
Nuint32_t u32NorDevOffset = 0;
N
N/*********************************************/
N
N
N/**********************************************
N*             Function definition             *
N**********************************************/
N
N/***************************************************************************/
Nstatic flash_errcodes_e eFLASH_CheckResult(uint16_t* pui16Addr, uint16_t ui16Expect)
N{
N    uint16_t ui16Status;
N    uint16_t u32StatusCnt = 0;
N
N    while( 1 )
N    {
N        ui16Status = *pui16Addr;
N        __DSB();
N
N        if( ui16Expect == (ui16Status & FLASH_DQ7) )
X        if( ui16Expect == (ui16Status & (0x80)) )
N        {
N            return FLASH_OK;
N        }
N        else
N        {
N            /* Completed? */
N            if( FLASH_DQ5 == (ui16Status & FLASH_DQ5) )
X            if( (0x20) == (ui16Status & (0x20)) )
N            {
N                /* Re-read the status */
N                ui16Status = *pui16Addr;
N                __DSB();
N
N                if( ui16Expect == (ui16Status & FLASH_DQ7) )
X                if( ui16Expect == (ui16Status & (0x80)) )
N                {
N                    return FLASH_OK;
N                }
N                else
N                {
N                    /* Timeout expired */
N                    return FLASH_TIMEOUT;  
N                }
N            }
N        }
N        /* Status indication */
N        u32StatusCnt++;
N        if( FLASH_ERASE_INDICATOR_INTERVAL == u32StatusCnt )
X        if( 0x19876UL == u32StatusCnt )
N        {
N            printf(".");
N            u32StatusCnt = 0;
N        }
N    }
N}
N
N
N/***************************************************************************/
Nstatic flash_errcodes_e eFLASH_EraseSector(uint32_t u32StartAddr)
N{
N    /* Send the erase command */
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA;  /* Unlock cycle 1 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055;  /* Unlock cycle 2 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x0080;  /* Setup command */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x0080;   
N    __DSB();
N
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA;  /* Unlock cycle 1 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055;  /* Unlock cycle 2 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;   
N    __DSB();
N    /* Start address range: 0x0000000 - 0x3FF0000 */
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + u32StartAddr) = 0x0030;    /* Sector erase command */ 
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + u32StartAddr) = 0x0030;      
N    __DSB();
N
N    return eFLASH_CheckResult((uint16_t*)(FLASH_BASE + u32NorDevOffset), FLASH_DQ7);
X    return eFLASH_CheckResult((uint16_t*)((((0x20000000))) + u32NorDevOffset), (0x80));
N}
N
N
N/***************************************************************************/
Nflash_errcodes_e eFLASH_Erase(void)
N{    
N    /* Send the erase command */
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA;  /* Unlock cycle 1 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055;  /* Unlock cycle 2 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x0080;  /* Setup command */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x0080;   
N    __DSB();
N
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA;  /* Unlock cycle 1 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055;  /* Unlock cycle 2 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x0010;  /* Chip erase command */ 
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x0010;    
N    __DSB();
N
N    return eFLASH_CheckResult((uint16_t*)(FLASH_BASE + u32NorDevOffset), FLASH_DQ7);
X    return eFLASH_CheckResult((uint16_t*)((((0x20000000))) + u32NorDevOffset), (0x80));
N}
N
N
N/***************************************************************************/
Nflash_errcodes_e eFLASH_EraseSectors(uint32_t u32DeviceAddr, uint32_t u32SectorCount)
N{    
N    flash_errcodes_e eRes = FLASH_OK;
N    uint32_t u32EraseIndex;
N
N    /* Erase the complete device */
N    if( 0 == u32SectorCount )
N    {
N        /* erase complete flash */
N        eFLASH_Erase();
N    }
N    else
N    {
N        /* Erase the require sectors only */
N        for(u32EraseIndex = 0; u32EraseIndex < u32SectorCount; u32EraseIndex++)
N        {
N            /* Erase sector */            
N            eRes = eFLASH_EraseSector(u32DeviceAddr + u32EraseIndex*FLASH_SECTOR_SIZE_128KB);           
X            eRes = eFLASH_EraseSector(u32DeviceAddr + u32EraseIndex*(128*1024));           
N            printf("Erase sector: %d\n", u32EraseIndex);
N        }
N    }
N
N    return eRes;
N}
N
N
N/***************************************************************************/
Nflash_errcodes_e eFLASH_Write(uint16_t* pui16Dest, uint16_t ui16Data)
N{    
N    /* Send the command */
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA; /* Unlock cycle 1 */ 
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055; /* Unlock cycle 2 */ 
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;   
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00A0; /* Setup command */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00A0;  
N    __DSB();
N
N    /* Send the data */
N    *pui16Dest = ui16Data;   
N    __DSB();
N
N    /* Poll the status */ 
N    return eFLASH_CheckResult(pui16Dest, (ui16Data & FLASH_DQ7));
X    return eFLASH_CheckResult(pui16Dest, (ui16Data & (0x80)));
N}
N
N
N/***************************************************************************/
Nflash_errcodes_e eFLASH_GetID(flash_devdescr_st* pstDeviceDesc)
N{
N    flash_errcodes_e eRetVal = FLASH_OK;
N
N    /* Exit autoselect/Reset command */
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset) = 0x00F0;
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset) = 0x00F0;
N    __DSB();
N
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x00AA; /* Unlock cycle 1 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x00AA;  
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_2AA) = 0x0055; /* Unlock cycle 2 */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0x554)) = 0x0055;  
N    __DSB();
N    *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + FLASH_ADDR_555) = 0x0090; /* Autoselect command */
X    *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (0xAAA)) = 0x0090;  
N    __DSB();
N
N    if( C_NULL != pstDeviceDesc )
X    if( ((void*)0) != pstDeviceDesc )
N    {    
N        pstDeviceDesc->ui16MID = *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + 0);
X        pstDeviceDesc->ui16MID = *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + 0);
N        __DSB();
N        pstDeviceDesc->ui16DID = *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset + (1<<1));
X        pstDeviceDesc->ui16DID = *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset + (1<<1));
N        __DSB();
N
N        /* Autoselect exit/Reset */
N        *(volatile uint16_t*)(FLASH_BASE + u32NorDevOffset) = 0x00F0;
X        *(volatile uint16_t*)((((0x20000000))) + u32NorDevOffset) = 0x00F0;
N        __DSB();
N
N        printf("FLASH Manufacturer ID: 0x%X\n", pstDeviceDesc->ui16MID);    
N        printf("FLASH Device ID: 0x%X\n", pstDeviceDesc->ui16DID);    
N        
N        /* !!! Add other manufacturer if needed !!! */
N        if( (0x0001 != (pstDeviceDesc->ui16MID & 0x00FF)) && (0x0020 != (pstDeviceDesc->ui16MID & 0x00FF)) )	
N        {
N            printf("\nERROR: Unsupported FLASH Device Manufacturer ID!\n");
N            eRetVal = FLASH_UNSUPPORTED_DEV;
N        }
N    }
N    else
N    {
N        eRetVal = FLASH_NULL_PTR;
N    }
N
N    return eRetVal;
N}
