N
N/**********************************************
N*                Include files                *
N**********************************************/
N
N/*********************************************/
N
N
N/**********************************************
N*                 Constants                   *
N**********************************************/
N
N/* Size of the device */
N#define FLASH_SIZE_NOR                      (128*1024*1024)
N#define FLASH_SECTOR_SIZE_128KB             (128*1024) 
N#define FLASH_MASK_128KB                    0xFFFE0000UL
N
N/* Base address according to the MMU table */
N#define FLASH_BASEMMU_CACHED	            (0x20000000)
N#define FLASH_BASEMMU_UNCACHED	            (0x20000000)
N
N/* Device base address, according to the NORIF settings */
N#define FLASH_BASE_CACHED		            (FLASH_BASEMMU_CACHED)
N#define FLASH_BASE_UNCACHED		            (FLASH_BASEMMU_UNCACHED)
N#define FLASH_END_CACHED                    (FLASH_BASE_CACHED + FLASH_SIZE_NOR)
N#define FLASH_END_UNCACHED		            (FLASH_BASE_UNCACHED + FLASH_SIZE_NOR)
N
N#define FLASH_BASE		                    (FLASH_BASE_UNCACHED)
N
N/* 
N *   Registers/commands/status polling 
N *   Word mode: A0 will be always kept "low", thus the
N *   command address has to be shifted left by one, i.e.
N *   A1 will be used as LSB ad the device can recognize 
N *   the sequence!
N */
N#define FLASH_ADDR_555	                    (0xAAA)     /* (0x555<<1) */
N#define FLASH_ADDR_2AA	                    (0x554)     /* (0x2AA<<1) */
N#define FLASH_DQ7		                    (0x80)
N#define FLASH_DQ5		                    (0x20)
N
N/*********************************************/
N
N
N/**********************************************
N*               Enumerations                  *
N**********************************************/
N/* Error codes */
Ntypedef enum tag_flash_errcodes_e
N{
N    FLASH_OK = 0,
N    FLASH_NULL_PTR,
N    FLASH_INVALID_PARAM,
N    FLASH_OPERATION_ERROR,
N    FLASH_TIMEOUT,
N    FLASH_UNSUPPORTED_DEV,
N    FLASH_UNKNWON_ERROR
N} flash_errcodes_e;
N
N/*********************************************/
N
N
N/**********************************************
N*                 Structures                  *
N**********************************************/
N/* Flash device descriptor */
Ntypedef struct tag_flash_devdescr_st 
N{                                         
N    uint16_t ui16MID;                       /* Manufacturer ID */
N    uint16_t ui16DID;                       /* Device ID */
N} flash_devdescr_st;
N
N/*********************************************/
N
N
N/**********************************************
N*        Exported function prototypes         *
N**********************************************/
N
N/****************************************************************************
N *
N * Function:      eFLASH_Erase
N *
N * Purpose:       Erease the entire device
N *
N * Inputs:        none
N *
N * Outputs:       none
N *
N * Return Values: Flash success/error code
N *
N * Limitations:   none
N *
N ***************************************************************************/
Nextern flash_errcodes_e eFLASH_Erase(void);
N
N
N/****************************************************************************
N *
N * Function:      eFLASH_EraseSectors
N *
N * Purpose:       Erase a number of sectors starting from specified address
N *
N * Inputs:        u32DeviceAddr:  Device physical address
N *                u32SectorCount: Sector count 
N *
N * Outputs:       none
N *
N * Return Values: Flash success/error code
N *
N * Limitations:   none
N *
N ***************************************************************************/
Nextern flash_errcodes_e eFLASH_EraseSectors(uint32_t u32DeviceAddr, uint32_t u32SectorCount);
N
N
N/****************************************************************************
N *
N * Function:      eFLASH_Write
N *
N * Purpose:       Write a single 16-bit data to the specified address
N *
N * Inputs:        pui16Dest: Address
N *                ui16Data:  Data 
N *
N * Outputs:       none
N *
N * Return Values: Flash success/error code
N *
N * Limitations:   none
N *
N ***************************************************************************/
Nextern flash_errcodes_e eFLASH_Write(uint16_t* pui16Dest, uint16_t ui16Data);
N
N
N/****************************************************************************
N *
N * Function:      eFLASH_GetID
N *
N * Purpose:       Retrieve device descriptor
N *
N * Inputs:        pstDeviceDesc: Empty decsriptor handle
N *
N * Outputs:       Device description
N *
N * Return Values: Flash success/error code
N *
N * Limitations:   none
N *
N ***************************************************************************/
Nextern flash_errcodes_e eFLASH_GetID(flash_devdescr_st* pstDeviceDesc);
